<!doctype html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.0.2/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-EVSTQN3/azprG1Anm3QDgpJLIm9Nao0Yz1ztcQTwFspd3yD65VohhpuuCOmLASjC" crossorigin="anonymous">
    <script src="https://code.jquery.com/jquery-3.6.0.min.js" integrity="sha256-/xUj+3OJU5yExlq6GSYGSHk7tPXikynS7ogEvDej/m4=" crossorigin="anonymous"></script>
    <script>
        searchPlayerFunc = (queryString, outId) => {
            console.log("I am here")
            fetch('http://localhost:3000/response/search?' + new URLSearchParams({queryString: queryString}), {method: 'GET'})
            .then(res => res.json())
            .then(data => {
                console.log(data)
                data.forEach(x => {
                    $(`<div class="form-check"><input class="form-check-input" type="radio" name="`.concat(String(outId), `" value="`.concat(String(x.x), `"><label class="form-check-label">`.concat(String(x.x), String(x.y), `</label></div>`)))).appendTo('#'.concat(outId))
                });
            })
        }
    </script>
</head>
