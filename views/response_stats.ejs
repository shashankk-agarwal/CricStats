<%- include('partials/head') %>
<body>
    <%- include('partials/header') %>
    <div class="container mt-5 col-5">
        <h6 class="text-center"><%= String(name) %> - Seasonwise Stats</h6>
        <div class="card bg-light">
            <% stats.forEach(stat => { %>
                <div class="card-header text-center">Season: <%= String(stat['Season']) %> </div>
                <ul class="list-group list-group-flush">
                    <% Object.keys(stat).forEach((key) => { %>
                        <% if ('Season' != key) { %>
                            <li class="list-group-item text-center"><%= String(key) + ": " + String(stat[key]) %></li>
                        <% } %>
                    <% }) %>
                </ul>
            <% }) %>
        </div>
    </div>
    <!-- <%- include('partials/footer') %> -->
</body>
